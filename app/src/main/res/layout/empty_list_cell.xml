<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>

        <variable
            name="obj"
            type="com.techonlabs.androidboilerplate.utils.recyclerView.EmptyListModel" />

        <variable
            name="lis"
            type="com.techonlabs.androidboilerplate.utils.recyclerView.OnRecyclerItemClickListener" />
    </data>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
